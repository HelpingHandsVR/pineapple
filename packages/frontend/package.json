{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn concurrently 'env-cmd nuxt-ts' 'yarn gen:watch'",
    "gen:watch": "graphql-codegen --config codegen.yml -w",
    "gen": "graphql-codegen --config codegen.yml",
    "build": "nuxt-ts build",
    "prebuild": "yarn gen",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "@asayerio/tracker": "^5.5.9",
    "@babel/runtime-corejs2": "^7.11.2",
    "@casl/vue": "^1.1.1",
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/apollo": "4.0.1-rc.3",
    "@nuxtjs/pwa": "^3.0.1",
    "@nuxtjs/sentry": "^4.3.4",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/apollo-composable": "4.0.0-alpha.10",
    "@vue/composition-api": "1.0.0-beta.10",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "apollo-link-batch-http": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "core-js": "3.6.5",
    "email-validator": "^2.0.4",
    "graphql-tag": "^2.11.0",
    "js-cookie": "^2.2.1",
    "luxon": "^1.24.1",
    "nuxt": "^2.14.3",
    "vuex-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/fragment-matcher": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-vue-apollo": "^1.17.9",
    "@nuxt/typescript-build": "^2.0.2",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@types/js-cookie": "^2.2.6",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0"
  }
}
